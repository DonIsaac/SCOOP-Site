{{#extend "application"}}
  {{#content "head"}}
    <link rel="stylesheet" href="assets/css/index.css" />
    <script src="assets/lib/ical/ical.min.js"></script>
  {{/content}}
  {{#content "body"}}
    <section class="bg-primary">
      <div class="container">
        <div class="row justify-content-center">
          <span class="border border-info">
            <iframe
              src="https://calendar.google.com/calendar/b/0/embed?height=600&amp;wkst=1&amp;bgcolor=%2333B679&amp;ctz=America%2FNew_York&amp;src=Yjd0bmJxY3JjYzhuM2NjdG84N2d2ZWVhbm9AZ3JvdXAuY2FsZW5kYXIuZ29vZ2xlLmNvbQ&amp;src=c2Nvb3B1bWRAZ21haWwuY29t&amp;color=%23F4511E&amp;color=%23D50000&amp;title=SCoop%20Events%20Calendar"
              style="border-width:0"
              width="800"
              height="600"
              frameborder="0"
              scrolling="no"
            ></iframe>
          </span>
          <script>
            setTimeout(() => {
        /* adjust some sizing to view better*/
        document.getElementsByTagName('iframe')[0].style.width = '50vw';
        // document.getElementsByTagName('iframe')[0].style.width = '800px';
        // document.getElementsByTagName('iframe')[0].style.width = '800px';
        document.getElementsByTagName('iframe')[0].style.height = '800px';

    }, 1000);
          </script>
        </div>
      </div>
    </section>
  {{/content}}
{{/extend}}