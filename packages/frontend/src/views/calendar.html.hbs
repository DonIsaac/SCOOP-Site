{{#extend "application"}}
  {{#content "head"}}
    <link rel="stylesheet" href="assets/css/index.css" />
    <script src="assets/lib/ical/ical.min.js"></script>
  {{/content}}
  {{#content "body"}}
    <section class="bg-primary">
      <div class="container">
        <div class="row justify-content-center">
          <span class="border border-info">
            <iframe class="rounded" src="https://calendar.google.com/calendar/embed?src=bjdab68hv62kakvi7rplgcvue4%40group.calendar.google.com&ctz=America%2FNew_York" 
            style="border: 0" 
            width="800" 
            height="600" 
            frameborder="0" 
            scrolling="no"></iframe>
          </span>
          <script>

            setTimeout(() => {
        /* adjust some sizing to view better*/
        document.getElementsByTagName('iframe')[0].style.width = '50vw';
        // document.getElementsByTagName('iframe')[0].style.width = '800px';
        // document.getElementsByTagName('iframe')[0].style.width = '800px';
        document.getElementsByTagName('iframe')[0].style.height = '800px';

    }, 1000);
          </script>
        </div>
      </div>
    </section>
  {{/content}}
{{/extend}}
