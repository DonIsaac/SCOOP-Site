{{#*inline "link"}}
  <a class="nav-link {{#if active}}active{{/if}}" href="{{url}}">{{@partial-block}}</a>
{{/inline}}

  <ul class="nav justify-content-end pb-3">
    {{#each pages}}
    <li class="nav-item">
      {{#if_eq this.text active }}
        {{#>link active=true url=this.url}}
          {{this.text}}
        {{/link}}
      {{else}}
        {{#>link url=this.url}}
          {{this.text}}
        {{/link}}
      {{/if_eq}}
    </li>
    {{/each}}
  </ul>
