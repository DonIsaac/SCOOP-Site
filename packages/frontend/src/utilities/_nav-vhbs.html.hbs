<!-- The navbar class is a navbar, it's a bar you navigate with -->
<ul class="nav navbar-nav justify-content-end pb-3">
    {{#each pages as |page|}}
    <li class="nav-item {{#if_eq page.text ../active}}active{{/if_eq}}">
        <a class="nav-link" href="{{page.url}}">{{page.text}}</a>
    </li>
    {{/each}}
</ul>